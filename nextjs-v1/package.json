  "name": "nextjs-v1-client",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "bun install --force && NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next build",
    "build-local": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && rm -rf node_modules && rm -rf bun.lockb",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "cross-env NEXT_CONFIG_FILE=next.config.electron.ts next build && electron-builder",
    "electron:build:mac": "cross-env NEXT_CONFIG_FILE=next.config.electron.ts next build && electron-builder --mac",
    "electron:build:win": "cross-env NEXT_CONFIG_FILE=next.config.electron.ts next build && electron-builder --win",
    "electron:build:linux": "cross-env NEXT_CONFIG_FILE=next.config.electron.ts next build && electron-builder --linux",
    "electron:start": "cross-env NODE_ENV=development concurrently \"next dev --turbopack\" \"wait-on http://localhost:3000/web-transc && electron .\""
  },
